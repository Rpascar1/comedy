<h1>All Jokes in Vault</h1>

  <% @jokes.each do |joke| %>
    <div>
      <label for="">Joke Topic:</label>
      <%= joke.topic %><br>
      <label for="">
        <bdi dir="rt l"></bdi>Body of joke: :</label><%= joke.body %><br>
        <label for=""><%= joke.id %></label><br>
        <label for=""><%= joke.user.name %></label><br>
      <%#= link_to 'Edit or Delete Joke', edit_joke_path(joke) %>
      <a href="/jokes/<%= joke.id %>/edit">Edit or Delete Joke<br></a>
      <label for="">-------------------------</label><br>

    </div>
  <% end %>
  <a href="/jokes/new">
    Create Joke
  </a><br>
  <br>

  <a href="/">Your Vault homepage</a><br>
