<h1>All Jokes in Vault</h1>
<% @jokes = current_user.jokes %>
  <% @jokes.each do |joke| %>
    <div>
      <label for="">Joke Topic:</label>
      <%= joke.topic %><br>
      <label for="">
        <bdi dir="rtl"></bdi>Body of joke: :</label><%= joke.body %><br>


      <a href="/jokes/<%= joke.id %>/edit">Edit or Delete Joke<br></a>
      <label for="">-------------------------</label><br>

    </div>
  <% end %>
  <a href="/jokes/new">
    Create Joke
  </a><br>
  <br>

  <a href="/">homepage</a><br>
<%= link_to "Logout", destroy_user_session_path, method: :delete %>
