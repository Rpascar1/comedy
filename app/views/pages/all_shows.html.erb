<% @shows = Show.all %>
  <% @shows.each do |show| %>
    <div>
      <%= show.club.name %><br> Presents<br>
      <%= show.user.email %> Performing <br>
      <%= show.name %>

      Show Date and time:<p><%= show.date.strftime ("%m/%d/%Y")%>, <%=show.date.strftime("%I:%M:%S %p") %>
    </div>
  <% end %>
